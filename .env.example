# API keys (comma-separated)
API_KEYS=dev_123,prod_abc

# Database: defaults to SQLite in project dir; for Postgres use e.g. postgresql+psycopg2://user:pass@host:5432/dbname
DATABASE_URL=sqlite:///./data.db

# Redis for background queue (optional). If empty, app uses FastAPI BackgroundTasks.
REDIS_URL=redis://localhost:6379/0

# Storage: if S3 is configured, files go to S3; else saved to ./uploads
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=ap-south-1
S3_BUCKET=

# Parser
ENABLE_OCR=true
MAX_FILE_MB=10
